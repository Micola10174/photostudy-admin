{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "eslint:recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "plugins": [
        "import",
        "react",
        "jsx-a11y",
        "jest"
    ],
    "rules": {
        "quotes": 0,
        "no-unused-vars": 0,
        "semi": [2, "never"],
        "no-confusing-arrow": ["error", {"allowParens": true}]
    }
}
